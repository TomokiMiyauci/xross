{
  "name": "xross-react",
  "version": "0.0.0",
  "description": "Component-based UI library for react",
  "main": "dist/xross-react.umd.js",
  "module": "dist/xross-react.es.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/xross-react.es.js",
      "require": "./dist/xross-react.umd.js"
    }
  },
  "license": "MIT",
  "scripts": {
    "commit": "yarn --cwd ../.. commit",
    "cz": "yarn commit",
    "dev": "vite",
    "lint": "run-p lint:*",
    "lint:tsc": "tsc --noEmit",
    "lint:script": "eslint src/**/*.{ts,tsx}",
    "lint:style": "stylelint src/*",
    "build": "run-p build:*",
    "build:lib": "vite build",
    "build:types": "tsc && tscpaths -p tsconfig.json -s src",
    "serve": "vite preview"
  },
  "dependencies": {
    "react": "^17.0.0",
    "react-dom": "^17.0.0"
  },
  "devDependencies": {
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@vitejs/plugin-react-refresh": "^1.3.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.23.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "vite": "^2.1.5"
  },
  "files": [
    "dist"
  ]
}
